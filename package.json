{
    "private": true,
    "main": "dist/main.js",
    "scripts": {
        "build": "rm -rf dist && tsc",
        "dev": "nodemon src/main.ts",
        "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
        "migration:generate": "yarn typeorm migration:generate -n",
        "migration:run": "yarn typeorm migration:run"
    },
    "dependencies": {
        "sqlite3": "^5.0.2",
        "typeorm": "^0.2.41",
        "reflect-metadata": "^0.1.13",
        "yargs": "^17.2.1",
        "@slack/bolt": "^3.8.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "typescript": "^4.5.2",
        "@types/node": "^16.11.10",
        "@types/sqlite3": "^3.1.7",
        "ts-node": "^10.4.0",
        "nodemon": "^2.0.15",
        "@types/yargs": "^17.0.7",
        "@types/uuid": "^8.3.3"
    }
}